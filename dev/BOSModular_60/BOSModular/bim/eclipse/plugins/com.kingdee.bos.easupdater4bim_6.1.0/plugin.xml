<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="com.kingdee.eas.updater"
   name="EASUpdater Plug-in"
   version="1.0.0"
   provider-name="">

   <runtime>
      <library name="commons-httpclient-2.0.2.jar">
         <export name="*"/>
      </library>
      <library name="commons-logging.jar">
         <export name="*"/>
      </library>
      <library name="commons-net-1.3.0.jar">
         <export name="*"/>
      </library>
      <library name="eas_updater-client.jar">
         <export name="*"/>
      </library>
      <library name="jakarta-oro-2.0.8.jar">
         <export name="*"/>
      </library>
      <library name="optional.jar">
         <export name="*"/>
      </library>
      <library name="bs_common.jar">
         <export
               name="*">
         </export>
      </library>
   </runtime>
   <requires>
      <import plugin="org.jdom"/>
      <import plugin="org.log4j" />
      <import plugin="com.kingdee.bos.ksql" />
   </requires>

</plugin>
