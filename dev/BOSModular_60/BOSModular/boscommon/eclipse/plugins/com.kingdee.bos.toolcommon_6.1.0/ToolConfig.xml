			<class>com.kingdee.eas.framework.client.service.UserMonitorService</class>
			<description>说明：</description>
			<compulsory>false</compulsory>
		</service>
		<service>
			<name>WorkFlow</name>
			<alias>工作流服务</alias>
			<class>com.kingdee.eas.framework.client.service.WorkFlowService</class>
			<description>说明：</description>
			<compulsory>false</compulsory>
			<defaultselected>true</defaultselected>
