		</action>
		<action>
			<alias>数据互斥</alias>
			<key>isObjectUpdateLock</key>
			<value>true</value>
			<description>说明：本配置为数据互斥所用，如有疑问，请与管理员联系。</description>
		</action>
		<action>
		     <alias>即时预警</alias>
		     <key>canForewarn</key>
		     <value>true</value>
		     <description>说明：本配置为即时预警所用，如有疑问，请与管理员联系。</description>
		</action>
	</actions>
	<!-- 定义客户端所用到的服务 -->
	<services>
		<service>
			<name>Permission</name>
