@css .bi_dialog_publish_portlet {padding:10px}
@css .bi_dialog_publish_portlet button{border:solid 1px #CCC;background-color:#EFF8FA;padding-top:3px;margin-left:10px;cursor:pointer}

<div class="bi_dialog_publish_portlet">
<div sue="msgDiv" style="padding:1px"></div>	
<form sue="portletForm" name="portletForm"  target="portlet" method="post" enctype="multipart/form-data">
<div style="padding-left:10px">
	<div sue="portletNumber" style="padding-top:10px;font:12px Arial">
		编码:
		<input name="portletNumber"  type="text" style="border:solid 1px #718BB7;width:116px;height:19px" value="PN-103-"/>
 		名称:
		<input name="portletName" type="text" style="border:solid 1px #718BB7;width:200px;height:19px" />
	</div>
	<div sue="portletDescription" style="padding-top:10px;font:12px Arial">
		<p>描述:
		  <textarea name="portletDescription" style="border:solid 1px #718BB7;width:350px;height:80px" ></textarea>
		</p>
	  </div>
</div>
	  <div id="portletIsTabDiv" sue="portletIsTab" align="left"  style="padding-top:10px;font:12px Arial">
		 <input name="portletIsTab" type="checkbox" value="checkbox">是否用页签显示
	  </div>
	  <div sue="btnconfirm" style="padding:10px;text-align:right;">
	  	<button sue="btnconfirm" name="btnconfirm"  class="portal_button">确定</button>
	  </div>
</form>
</div>