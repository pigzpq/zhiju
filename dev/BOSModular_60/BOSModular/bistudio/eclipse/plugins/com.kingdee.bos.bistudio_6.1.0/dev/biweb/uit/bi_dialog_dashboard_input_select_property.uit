<table cellpadding="0" cellspacing="0" style="font-size:12px;">
	<tr style="height:35px">
		<td style="width:100px;text-align:right">对象名称：</td>
		<td>
			<input sue="objectName" type="text"/>
		</td>
	</tr>
	<tr style="height:35px">
		<td style="width:100px;text-align:right">标签：</td>
		<td>
			<input sue="hintText" type="text"/>
		</td>
	</tr>
	<tr style="height:35px">
		<td style="width:100px;text-align:right">宽度：</td>
		<td>
			<input sue="width" text="100" type="text"/>像素
		</td>
	</tr>
	<tr style="height:35px">
		<td style="width:100px;text-align:right">数据源选择：</td>
		<td>
			<select sue="chooseDS">
				<option value="">请选择数据源...</option>
			</select>
		</td>
	</tr>
	<tr style="height:35px">
		<td style="width:100px;text-align:right">名称列：</td>
		<td>
			<select sue="captionColumn" style="width:100px"></select>
		</td>
	</tr>
	<tr style="height:35px">
		<td style="width:100px;text-align:right">值列：</td>
		<td>
			<select sue="valueColumn" style="width:100px"></select>
		</td>
	</tr>
</table>