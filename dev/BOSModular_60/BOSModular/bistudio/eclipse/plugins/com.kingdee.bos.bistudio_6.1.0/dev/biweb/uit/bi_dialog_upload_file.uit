<div>
	<div sue="msgDiv"></div>
	<div sue="divRepAction" style="padding:10px 0px;text-align:right">
		<button sue="btnNewDir" class="portal_button" style="margin-right:10px">新建</button>
		<button sue="btnDel" class="portal_button">删除</button>
	</div>
	<div sue="repTree" style="width:100%;height:135px;border:solid 1px #718BB7;overflow-y:auto"></div>
	<form sue="form" target="uploader" method="post" enctype="multipart/form-data">
